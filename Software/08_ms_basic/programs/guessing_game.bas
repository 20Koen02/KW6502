100 PRINT "GUESS THE NUMBER BETWEEN 1 AND 100."
110 LET X = INT(100*RND(0)+1)
120 LET N = 0
130 PRINT "YOUR GUESS";
140 INPUT G
150 LET N = N+1
160 IF G = X THEN 300
170 IF G < X THEN 250
180 PRINT "TOO LARGE, GUESS AGAIN"
190 GOTO 160
200 PRINT "TOO SMALL, GUESS AGAIN"
210 GOTO 160
220 PRINT "YOU GUESSED IT, IN"; N; "TRIES"
230 PRINT "ANOTHER GAME (YES = 1, NO = 0)";
240 INPUT A
250 IF A = 1 THEN 140
260 PRINT "THANKS FOR PLAYING"